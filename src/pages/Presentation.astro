---
const { tipo, id } = Astro.props;

let tipoInvitacion;

switch(tipo) {
    case 'I':
    case 'i':
        tipoInvitacion = "invitados";
        break;
    case 'F':
    case 'f':
        tipoInvitacion = "familias";
        break;
    default:
        tipoInvitacion = "invitados";
} 
let invitado;
if(tipo && id){
    invitado = await fetch(`https://wedding-back.fly.dev/${tipoInvitacion}/${id}`);
}
const invitadoInfo = await invitado?.json();
